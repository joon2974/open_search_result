{
  "manifest_version": 2,
  "name": "open search results",
  "description": "chrome extension for opening search results",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default:icon": "icon.png"
  },

  "permissions": [
    "https://*/*",
    "http://*/*/",
    "tabs",
    "activeTab"
  ]
}
